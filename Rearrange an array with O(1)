#include<iostream>
using namespace std;
int main()
 {
    int n;
    cin>>n;
    while(n--)
    {
        int l; 
        cin>>l;
        int a[l];
        int arr[l];
        for(int i=0; i<l; i++)
            cin>>a[i];
        for(int j=0; j<l; j++)
            arr[j]= a[a[j]];
        for(int i=0; i<l; i++)
            cout<<arr[i]<<" ";
        cout<<endl;
    }
    
	//code
	return 0;
}
