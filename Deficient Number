#include<iostream>
using namespace std;
int main()
 {
     int n;
     cin>>n;
     while(n--)
     {
         int l;
         cin>>l;
         int sum=0;
         for(int i=1; i<=l; i++)
        {
            if(l%i==0)
                sum+= i;
        }
        if(sum<(2*l))
            cout<<"1"<<endl;
        else
            cout<<"0"<<endl;
     }
	//code
	return 0;
}
