#include<iostream>
using namespace std;
int main()
 {
     int n;
     cin>>n;
     while(n--)
     {
         int x1,x2,x3,x4,y1,y2,y3,y4;
         cin>>x1>>y1>>x2>>y2;
         cin>>x3>>y3>>x4>>y4;
         if(x1>x4||x2<x3||y1<y4||y2>y3)
            cout<<"0"<<endl;
        else
            cout<<"1"<<endl;
     }
	//code
	return 0;
}
