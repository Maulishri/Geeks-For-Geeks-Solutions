#include <iostream>
using namespace std;

int main() {

	//code
	int p;
	cin>>p;
	while(p>0)
	{
	    long n,b;
	    cin>>n>>b;
	    long a[n];
	    for(int i=0; i<n; i++)
	        cin>>a[i];
	    for(int i=0; i<n; i++)
	    {
	        if(a[i]==b)
	        {
	            b*=2;
	        }
	    }
	    cout<<b<<endl;
	    p--;
	    
	}
	return 0;
}
